%h1.h2= t('.title')

.card
  = form_with url: { controller: :import_expenses, action: :create }, local: true do |f|
    .card-body
      .form-group
        = f.label :import_string
        = f.text_area :import_string, class: 'form-control'
    .card-footer
      = f.submit t('.submit'), class: 'btn btn-primary'
